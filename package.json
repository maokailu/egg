  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json --copy-files",
    "dev:server": "egg-scripts start --daemon --title=egg-server-egg",
    "stop": "egg-scripts stop --title=egg-server-egg",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "start:client": "webpack-dev-server --inline --hot",
    "server:client": "webpack-dev-server --config ./config/webpack.config.server.js",
    "build:client": "set NODE_ENV=production && webpack --config ./config/webpack.config.prod.js",
    "dev:client": "webpack --watch --config ./config/webpack.config.server.js"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFiles": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "./src/components/*"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": ".tests/fileMock.js",
      "\\.(css|less)$": ".tests/styleMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    }
  },
